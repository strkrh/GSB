<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".parametres">

    <include layout="@layout/entete"/>
    <TextView
        android:labelFor="@+id/TV_Parametres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="80dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Parametres"
        android:textColor="#0D3C0D"
        android:textSize="40dp" />
    <TextView
        android:labelFor="@+id/TV_E_mail3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="E-mail"
        android:textColor="#243103"
        android:textSize="19dp" />
<EditText
        android:id="@+id/email_Parametres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />
    <TextView
        android:labelFor="@+id/TV_CodeVisiteur2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Code Visiteur :"
        android:textColor="#243103"
        android:textSize="19dp" />

    <TextView
        android:labelFor="@+id/TV_Nom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Nom :"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/Nom_Parametres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />
    <TextView
        android:labelFor="@+id/TV_Prenom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Prenom :"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/Prenom_Parametres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />
    <TextView
        android:labelFor="@+id/TV_Url"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Url"
        android:textColor="#243103"
        android:textSize="19dp" />
    <TextView
        android:labelFor="@+id/TV_Motdepasse3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Mot de passe"
        android:textColor="#243103"
        android:textSize="19dp" />
    <EditText
        android:id="@+id/Motdepasse_Parametres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:background="#E9FDB7"
        android:hint=""
        android:textColor="#CCEA7D" />

    <Button
        android:id="@+id/main_button_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:backgroundTint="#B5E180"
        android:fontFamily="@font/quicksand_medium"
        android:text="Confirmer"
        android:textColor="#233023"
        app:cornerRadius="5dp" />


</LinearLayout>